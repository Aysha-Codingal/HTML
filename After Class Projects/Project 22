    <div id="res"></div>
    <script>
    function findmax(){
        var i,max; 
        max = 0
        for(var i = 0; i< arguments.length ; i++)
        {
            if (arguments[i] > max)
        {
            max = arguments [i]
        }
        }
        return max
    }
    
    document.getElementById("res").innerHTML = "the greatest value is " + findmax (23,45,96,247,247,23,3457,707,889)
    </script>

